{% extends 'PasswordGeneratorBundle::layout.html.twig' %}

{% block layout %}
    {{ form_start(form) }}
        {{ form(form) }}
        <input type="submit" value="Generate">
    {{ form_end(form) }}

    {% if passwords %}
        {% for password in passwords %}
            {{ password }}<br>
        {% endfor %}
    {% endif %}
{% endblock layout %}
